{
  "scripts": {
    "setup": "npm install && npm install --prefix src/",
    "cdk-bootstrap": "npx cdk bootstrap",
    "cdk-synth": "npx cdk synth presigned-url-dev",
    "cdk-deploy": "npx cdk deploy presigned-url-dev --outputs-file ../src/cdk-exports-dev.json",
    "cdk-create-stack": "npm run cdk-bootstrap && npm run cdk-deploy",
    "cdk-destroy": "npx cdk destroy presigned-url-dev",
    "test": "jest",
    "watch": "tsc -w",
    "build": "tsc",
    "cdk": "cdk",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@aws-cdk/assert": "1.95.1",
    "@aws-cdk/aws-apigatewayv2": "1.95.1",
    "@aws-cdk/aws-apigatewayv2-integrations": "1.95.1",
    "@aws-cdk/aws-iam": "1.95.1",
    "@aws-cdk/aws-lambda": "1.95.1",
    "@aws-cdk/aws-lambda-nodejs": "1.95.1",
    "@aws-cdk/aws-logs": "1.95.1",
    "@aws-cdk/aws-s3": "1.95.1",
    "@aws-cdk/core": "1.95.1",
    "aws-cdk": "1.95.1",
    "source-map-support": "^0.5.19",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "esbuild": "^0.8.34",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.4.2",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "name": "aws-s3-uploads-react-cdk",
  "version": "1.0.0",
  "description": "How to upload files to s3 using presigned urls, infrastructure is provisioned using CDK",
  "keywords": [
    "aws",
    "cdk",
    "typescript",
    "lambda"
